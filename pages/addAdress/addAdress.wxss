.addAdress{
    background-color: #f2f7f8;
    min-height: 1218rpx;
}
/*内容*/
.addAdress_part{
    margin-top: 28rpx;
    background-color: #fff;
}
.addAdress_part_item{
    height: 100rpx;
    padding: 0 28rpx;
    border-bottom: 1px solid #e4e4e4;
    font-size: 28rpx;
    display: flex;
    align-items: center;
}
.addAdress_part_item text{
  min-width: 140rpx;
  text-align:right;
}
.addAdress_part_item_right{
    justify-content: space-between;
}
.addAdress_part_item_circle_d{
    height: 40rpx;
    width: 40rpx;
    flex-shrink: 0;
    border-radius: 20rpx;
    border: 1px solid #e4e4e4;
    background-color: #f5f5f5;
}
.addAdress_part_item_circle{
    height: 40rpx;
    width: 40rpx;
    border-radius: 20rpx;
    border: 1px solid #e4e4e4;
    background-color: #f5f5f5;
}

/*按钮*/
.addAdress_button{
    display: flex;
    justify-content: center;
}
.addAdress_button_item{
    width: 618rpx;
    height: 74rpx;
    background-color: #fe9204;
    border-radius: 10rpx;
    color: #fff;
    font-size: 30rpx;
    display: flex;
    justify-content: center;
    align-items: center;
    margin-top: 220rpx;
}
.addressInput{
    width:600rpx
}